{
	services: [
		{
			name: "Piped"
			cmd: "ping"
			target: "127.0.0.1:14570"
			interval: "2m"
			retry: 3
			retry_interval: "1s"
			read_timeout: "2s"
           			notification_cmds: ["email_developer"]
		},
	]
	
	notifications: [
		{
			name: "email_developer"
			cmd: "email"
			server: "****.***.***:**"
			user: "******@***.***"
			pwd: "********"
			notifiers: ["******@***.***"]
		},
	]
}
